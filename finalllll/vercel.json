{
  "version": 2,
  "builds": [
    {
      "src": "index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "index.js"
    }
  ],
  "functions": {
    "api/news": {
      "runtime": "python3.9",
      "handler": "news.py",
      "maxDuration": 10
    }
  },
  "env": {
    "NODE_ENV": "production"
  }
}
